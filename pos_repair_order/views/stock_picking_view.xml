<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_picking_repair_inherited_view_form" model="ir.ui.view">
            <field name="name">stock.picking.repair.form.view.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="repair_obj_id"/>
                </xpath>
            </field>
        </record>
  </data>
</odoo>